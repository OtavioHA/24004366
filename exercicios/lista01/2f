#include <stdio.h>

int main() {

    double fahrenheit;
    double celsius; 

    printf("Digite a temperatura em graus Fahrenheit: ");
    scanf("%lf", &fahrenheit);

    celsius = (5.0 / 9.0) * (fahrenheit - 32);

    printf("A temperatura em graus Celsius Ã©: %.2lf\n", celsius);

    return 0;
}
