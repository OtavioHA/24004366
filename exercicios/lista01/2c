#include <stdio.h>

int main() {
    char nome[100];
    int idade;  

    printf("Qual é o seu nome?\n");
    scanf("%99s", nome); 

    printf("Qual sua idade ?\n");
    scanf("%d", &idade);

    int dias = idade * 365;

    printf("Olá, %s! Você tem aproximadamente %d dias de vida.\n", nome, dias);

    return 0;
}
